# アプリケーション設定
APP_ENV=testing
DEBUG=true
PROJECT_NAME=UniCore
API_V1_PREFIX=/api/v1

# セキュリティ設定
SECRET_KEY=your-test-secret-key
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# CORS設定
BACKEND_CORS_ORIGINS='["http://localhost:3000","http://localhost:8080"]'

# PostgreSQL接続設定（テスト用）
POSTGRES_SERVER=localhost
POSTGRES_USER=unicore_test_user
POSTGRES_PASSWORD=your_test_password
POSTGRES_DB=unicore_test
POSTGRES_PORT=5432

# SQLAlchemy設定（テスト用）
DATABASE_URL=postgresql+asyncpg://unicore_test_user:your_test_password@localhost:5432/unicore_test
SYNC_DATABASE_URL=postgresql://unicore_test_user:your_test_password@localhost:5432/unicore_test

# 初期管理者ユーザー設定（テスト用）
FIRST_SUPERUSER_EMAIL=admin_test@example.com
FIRST_SUPERUSER_PASSWORD=admintestpassword

# Docker環境設定
AUTO_MIGRATE=true